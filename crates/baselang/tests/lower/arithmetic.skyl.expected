Test "arithmetic"
  Assert
    Cmp(Eq)
      BinOp(Add)
        Int(2)
        Int(2)
      Int(4)
  Assert
    Cmp(Eq)
      BinOp(Mul)
        Int(3)
        Int(4)
      Int(12)
  Assert
    Cmp(Eq)
      BinOp(Sub)
        Int(10)
        Int(3)
      Int(7)
  Assert
    Cmp(Eq)
      BinOp(Div)
        Int(20)
        Int(4)
      Int(5)
